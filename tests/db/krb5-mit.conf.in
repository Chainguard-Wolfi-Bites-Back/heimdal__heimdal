[libdefaults]
	default_realm = EXAMPLE.ORG
        ccapi_library = /no/such/file

[kdc]
	database = {
		label = {
			realm = EXAMPLE.ORG
			dbname = mit-db:@srcdir@/mit-db
			mkey_file = @srcdir@/mit-db-master-stash
                        log_file = @objdir@/current.log
		}
	}

[hdb]
	db-dir = @objdir@

[logging]
	default = 0-/FILE:@objdir@/messages.log
